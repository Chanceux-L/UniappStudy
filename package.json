    "deploy:staging:mp-weixin": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service deploy:mp-weixin --mode staging",
    "deploy:production:mp-weixin": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service deploy:mp-weixin --mode production",
    "deploy:staging:h5": "cross-env NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service deploy:h5 --mode staging",
    "deploy:production:h5": "cross-env NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service deploy:h5 --mode production",
    "info": "node node_modules/@dcloudio/vue-cli-plugin-uni/commands/info.js",
    "lint": "vue-cli-service lint src --no-fix --max-warnings 0",
    "lint:style": "cross-env UNI_PLATFORM=mp-weixin vue-cli-service lint:style --no-fix --max-warnings 0",
    "lint:style:fix": "cross-env UNI_PLATFORM=mp-weixin vue-cli-service lint:style"
